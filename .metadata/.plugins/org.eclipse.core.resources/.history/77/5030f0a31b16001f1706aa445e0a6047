package negocio;

import java.util.List;

import datos.Evento;

public class EventoABM {
	public static EventoABM instance;
	
	private EventoABM() {};
	
	private static EventoABM getInstance() {
		if(instance == null) {
			instance = new EventoABM();
		}
		return instance;
	}
	
	public List<Evento> traer(){
		return EventoABM.getInstance().traer();
	}
	
}
